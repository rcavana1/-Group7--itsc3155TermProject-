<h1>New Expense</h1>
<%= form_for :budget, url: expenses_path do |f| %>
     <p>
        <%= f.label :name_of_Expense %><br>
        <%= f.text_field :name_of_Expense %>
    </p>
  <p>
    <%= f.label :amount %><br>
    <%= f.number_field :amount %>
  </p>
  
    <p>
    <%= f.label :date %><br>
    <%= f.date_field :date %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
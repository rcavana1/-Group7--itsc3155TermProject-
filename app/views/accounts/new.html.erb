<h1>New Account</h1>
<%= form_for :account, url: accounts_path do |f| %>
    <p>
        <%= f.label :username %><br>
        <%= f.text_field :username %>
    </p>
    <p>
        <%= f.label :password %><br>
        <%= f.text_field :password %>
    </p>
    <p>
        <%= f.label :name %><br>
        <%= f.text_field :name %>
    </p>
    <p>
        <%= f.submit %>
    </p>
<% end %>